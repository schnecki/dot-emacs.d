;; ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
;; ++++++++++++++++++++++++++++ EMACS LISP  +++++++++++++++++++++++++++++
;; ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++


;; KEYS

(defun emacs-lisp-keys ()
  (local-set-key (kbd "C-x x e r") 'eval-region)
  (local-set-key (kbd "C-x x e b") 'eval-buffer)
  )

(add-hook 'emacs-lisp-mode-hook 'emacs-lisp-keys)
