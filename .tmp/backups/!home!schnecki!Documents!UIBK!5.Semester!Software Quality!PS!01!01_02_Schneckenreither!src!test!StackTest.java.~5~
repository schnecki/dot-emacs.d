
package qm.test;

import java.util.Random;
import java.util.Stack;

import org.junit.Test;
import org.junit.runner.RunWith;
import org.junit.runners.JUnit4;


/**
 *  Unit Test for class Stack
 *
 *
 * Created: Tue Oct 15 20:00:32 2013
 *
 * @author <a href="mailto:manuel.schneckenreither@student.uibk.ac.at">Manuel Schneckenreither</a>
 * @version 1.0
 */
@RunWith(JUnit4.class)
public class StackTest {

    /**
     * Test for method isEmpty
     */
    @Test
    public void isEmptyTest() {
        Stack stack = new Stack(10);

        assertTrue(stack.istEmpty());

        stack.push(new Random().nextInt(4));
            
        
    } // isEmptyTest

}

