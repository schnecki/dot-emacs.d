        {Annotated Types};

        \node [text width=4.5cm,text centered,minimum width=1cm,minimum height=1.5em,outer
        sep=0pt,draw=black,fill=blue!10,semithick] at (0,1) (C) {Costs
          (Runtime)};

        % arrows
        \draw [->,shorten >=2pt,shorten <=2pt,semithick] (A.south) --
        +(0,-1.7em) -| (B) node [right,midway] {\footnotesize Amortised\,Analysis};
        \draw [->,shorten >=2pt,shorten <=2pt,semithick] (B.south) --
        +(0,-1.7em) -| (C) node [right,midway] {\footnotesize
          Adjusted Type Inference of Paper};



      \end{tikzpicture}

    \end{frame}
